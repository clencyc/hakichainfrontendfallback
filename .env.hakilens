# HakiLens Python Backend Integration
# Add these variables to your .env file

# Python Backend URL (your existing backend)
VITE_PYTHON_BACKEND_URL=http://localhost:5007

# Node.js Backend URL (fallback backend)
VITE_NODEJS_BACKEND_URL=http://localhost:8001

# HakiLens API Configuration
VITE_HAKILENS_API_URL=http://localhost:5007

# Enable debug mode for API calls
VITE_DEBUG_API=true

# Mobile features configuration
VITE_MOBILE_FEATURES_ENABLED=true

# Offline sync settings
VITE_OFFLINE_SYNC_LIMIT=50
VITE_OFFLINE_CACHE_DURATION=86400000

# Voice search settings
VITE_VOICE_CONFIDENCE_THRESHOLD=0.7

# Search performance settings
VITE_SEARCH_TIMEOUT=10000
VITE_QUICK_SEARCH_LIMIT=10
